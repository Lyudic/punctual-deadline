#include "iostream"
#include <cmath>

using namespace std;

int main(){
    float x;
    cout <<"Solve function.\n" \\Решить функцию...
         <<"Enter:" ;
    cin  >> x;  //В условии х = −2.34; Ответ:-1.76911 

    float f = (abs(x-5)-sin(x))/3 + (sqrt(x*x+2014))*cos(2*x) - 3;
    cout << " x = " << x <<endl
         << " f = " << f <<endl;

    system("pause");
    return 0;
}
